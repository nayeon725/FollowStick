<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.follow_stick_52">

    <!-- 위치 권한 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_BACKGROUND_LOCATION" />
    <!-- 블루투스 권한 -->
    <uses-permission android:name="android.permission.BLUETOOTH_CONNECT" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <!-- tts 권한 -->
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <!-- 카카오 API 권한 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Follow Stick"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Follow_Stick_52"
        tools:targetApi="31">
        <activity
            android:name=".uiFPage"
            android:exported="false" />
        <activity
            android:name=".toiletFPage"
            android:exported="false" />
        <activity
            android:name=".subFPage"
            android:exported="false" />
        <activity
            android:name=".settingFPage"
            android:exported="false" />
        <activity
            android:name=".settingGuideFPage"
            android:exported="false" />
        <activity
            android:name=".secondFPage"
            android:exported="false" />
        <activity
            android:name=".ledFPage"
            android:exported="false" />
        <activity
            android:name=".guideFPage"
            android:exported="false" />
        <activity
            android:name=".functionFPage"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>